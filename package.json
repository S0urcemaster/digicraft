{
  "name": "digicraft",
  "version": "1.0.0",
  "description": "The Digi Craft Project",
  "author": "sntr",
  "private": true,
  "scripts": {
    "build": "pnpm run -r build",
    "build:packages": "pnpm -F ./packages/** build",
    "build:lib": "pnpm -F @digicraft/lib build",
    "build:context": "pnpm -F @digicraft/context build",
    "build:svg": "pnpm -F @digicraft/svg build",
    "build:ui": "pnpm -F @digicraft/ui build",
    "build:dc3": "pnpm -F @digicraft/digicraft3 build",
    "build:dc2": "pnpm -F @digicraft/digicraft2 build",
    "build:dc2server": "pnpm -F @digicraft/digicraft2server build",
    "clean": "pnpm  -F ./packages/** clean",
    "dev": "pnpm -F @digicraft/digicraft3 run dev",
    "dev:dc2": "pnpm -F @digicraft/digicraft2 run dev",
    "start": "pnpm -F @digicraft/digicraft3 start",
    "start:dc2": "pnpm -F @digicraft/digicraft2 start"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "typescript": "~4.9.5"
  },
  "dependencies": {
  },
  "keywords": [],
  "license": "ISC"
}
